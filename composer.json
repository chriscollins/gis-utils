{
    "name": "chriscollins/gis-utils",
    "description": "A PHP library for performing GIS tasks, such as geocoding addresses and transforming coordinates.",
    "keywords": ["GIS", "Geocode", "Mapping", "Latitude", "Longitude", "Datum", "Postcode"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Collins",
            "email": "chrisjamescollins@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "pimple/pimple": "~3.5",
        "chriscollins/general-utils": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.22",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "autoload": {
        "psr-4": { "ChrisCollins\\GisUtils\\": "lib/ChrisCollins/GisUtils" }
    },
    "autoload-dev": {
        "psr-4": { "ChrisCollins\\GisUtils\\Test\\": "test/ChrisCollins/GisUtils/Test" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
