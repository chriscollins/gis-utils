{
    "name": "chriscollins/gis-utils",
    "description": "A PHP library for performing GIS tasks, such as geocoding addresses and transforming coordinates.",
    "keywords": ["GIS", "Geocode", "Mapping", "Latitude", "Longitude", "Datum", "Postcode"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Collins",
            "email": "chrisjamescollins@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "pimple/pimple": "~1.0.2",
        "chriscollins/general-utils": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "squizlabs/php_codesniffer": "~1.5",
        "phpmd/phpmd": "~2.0"
    },
    "autoload": {
        "psr-0": { "": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
